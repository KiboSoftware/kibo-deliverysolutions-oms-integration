
{
  "compilerOptions": {
    "target": "es6",
    "module": "node16",
    "strict": true,
    "moduleResolution": "Node16",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "rootDir": "src",
    "outDir": "build",
    "declaration": true,
    "sourceMap": true,
    "strictNullChecks":false,
    "resolveJsonModule": true,
    "noImplicitAny": false
  }
}